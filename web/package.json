{
	"name": "web_template",
	"version": "v0.4.4-web_template-dev",
	"description": "This is a general web template repository of Kenneth Trecy Tobias. Its purpose is to be used for other templates/projects either by forking this repository, copying its files, or merging its history to other existing templates/projects.",
	"homepage": "https://github.com/KennethTrecy/web_template",
	"keywords": [
		"front-end",
		"web",
		"template-repository"
	],
	"private": "true",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"dev": "npm run development",
		"development": "npx rollup --bundleConfigAsCjs -c bundler/dev/rollup.config.js",
		"watch": "npx rollup --bundleConfigAsCjs -c bundler/dev/rollup.config.js --watch",
		"prod": "npm run production",
		"production": "npx rollup --bundleConfigAsCjs -c bundler/prod/rollup.config.js",
		"test:unit": "npx jest",
		"test:watch_unit": "npx jest --watch"
	},
	"dependencies": {
		"vue": "^3.2.29"
	},
	"devDependencies": {
		"@babel/core": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@rollup/plugin-alias": "^4.0.2",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.2.1",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/test-utils": "^2.2.6",
		"@vue/vue3-jest": "^29.2.2",
		"babel-jest": "^29.3.1",
		"changelogen": "^0.4.0",
		"comroconbu": "github:KennethTrecy/comroconbu#v0.2.0",
		"husky": "^8.0.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"jsdom": "^20.0.3",
		"regenerator-runtime": "^0.13.9",
		"rollup-plugin-scss": "^4.0.0",
		"rollup": "^3.8.1",
		"sass": "^1.49.7",
		"vite": "^4.0.3"
	}
}
