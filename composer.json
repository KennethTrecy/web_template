{
	"name": "kennethtrecy/web_template",
	"type": "library",
	"description": "A simple web template for general reuse.",
	"keywords": ["back-end", "web", "template-repository"],
	"homepage": "https://github.com/KennethTrecy/web_template",
	"license": "MIT",
	"authors": [
		{
			"name": "Kenneth Trecy Tobias",
			"email": "web.kennethtobias@gmail.com",
			"homepage": "https://github.com/KennethTrecy",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.0.2"
	},
	"require-dev": {
		"laravel/framework": "^9.0",
		"orchestra/testbench": "^7.0",
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"KennethTrecy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "t/"
		}
	},
	"scripts": {
		"test:offline:fast": [
			"./vendor/bin/phpunit --testsuite Unit,Feature"
		],
		"test:offline:slow": [
			"./vendor/bin/phpunit --testsuite Integration"
		],
		"test:offline:all": [
			"./vendor/bin/phpunit"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"secure-http": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"KennethTrecy\\"
			]
		}
	}
}
