{
	"name": "kennethtrecy/web_template",
	"type": "library",
	"description": "A simple web template for general reuse.",
	"keywords": ["back-end", "web", "template-repository"],
	"homepage": "https://github.com/KennethTrecy/web_template",
	"license": "MIT",
	"authors": [
		{
			"name": "Kenneth Trecy Tobias",
			"email": "web.kennethtobias@gmail.com",
			"homepage": "https://github.com/KennethTrecy",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.3|^8.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"autoload": {
		"psr-4": {
			"KennethTrecy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "t/"
		}
	},
	"scripts": {
		"coverage:offline:fast": [
			"composer run test:offline:fast -- --coverage-html hidden_coverage"
		],
		"coverage:offline:slow": [
			"composer run test:offline:slow -- --coverage-html hidden_coverage"
		],
		"coverage:offline:all": [
			"composer run test:offline:all -- --coverage-html hidden_coverage"
		],
		"test:offline:fast": [
			"./vendor/bin/phpunit --testsuite Unit,Feature"
		],
		"test:offline:slow": [
			"./vendor/bin/phpunit --testsuite Integration"
		],
		"test:offline:all": [
			"./vendor/bin/phpunit"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"secure-http": true
	}
}
