{
	"name": "web_template",
	"description": "This is a general web template repository of Kenneth Trecy Tobias. Its purpose is to be used for other templates/projects either by forking this repository, copying its files, or merging its history to other existing templates/projects.",
	"homepage": "https://github.com/KennethTrecy/web_template",
	"keywords": [
		"front-end",
		"web",
		"template-repository"
	],
	"type": "module",
	"private": "true",
	"license": "MIT",
	"scripts": {
		"prepare": "husky",
		"dev": "npm run development",
		"development": "npx rollup -c bundler/dev/rollup.config.js",
		"watch": "npx rollup -c bundler/dev/rollup.config.js --watch",
		"prod": "npm run production",
		"production": "npx rollup -c bundler/prod/rollup.config.js",
		"test:unit": "npx jest",
		"test:watch_unit": "npx jest --watch"
	},
	"dependencies": {
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/test-utils": "^2.2.6",
		"@vue/vue3-jest": "^29.2.2",
		"babel-jest": "^29.3.1",
		"changelogen": "^0.5.7",
		"esbuild": "^0.20.0",
		"husky": "^9.1.7",
		"jest-environment-jsdom": "^29.3.1",
		"jest": "^29.7.0",
		"jsdom": "^20.0.3",
		"regenerator-runtime": "^0.14.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup": "^4.12.0"
	}
}
