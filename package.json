{
	"name": "web_template",
	"version": "v0.4.4-web_template-dev",
	"description": "This is a general web template repository of Kenneth Trecy Tobias. Its purpose is to be used for other templates/projects either by forking this repository, copying its files, or merging its history to other existing templates/projects.",
	"homepage": "https://github.com/KennethTrecy/web_template",
	"keywords": ["front-end", "web", "template-repository"],
	"private": "true",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"dev": "npm run development",
		"development": "echo development bundler command here",
		"watch": "echo watched development bundler command here",
		"prod": "npm run production",
		"production": "npx rollup -c bundler/prod/rollup.config.js",
		"test:unit": "npx jest",
		"test:watch_unit": "npx jest --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"changelogen": "^0.4.0",
		"husky": "^8.0.0",
		"jest": "^29.3.1",
		"regenerator-runtime": "^0.13.9",
		"rollup": "^2.67.0"
	}
}
